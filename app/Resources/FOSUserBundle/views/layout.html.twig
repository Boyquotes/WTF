<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    {% block stylesheets %}
        <link href="https://fonts.googleapis.com/css?family=Play|Press+Start+2P" rel="stylesheet">
        {% stylesheets 'bundles/wtf/css/*' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />{% endstylesheets %}
    {% endblock %}
</head>
<body>
    {% include('header.html.twig') %}

{% if app.request.hasPreviousSession %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}

<div>
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>

{% block javascripts %}{% javascripts
'web/public/components/jquery/jquery.js'
'web/public/js/*' %}
    <script src="{{ asset_url }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&libraries=places"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
{% endjavascripts %}{% endblock %}

{% include('footer.html.twig') %}

</body>
</html>
